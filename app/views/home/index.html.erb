


<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Object</th>
      <th scope="col">Content</th>
    </tr>
  </thead>
  <tbody>
  <%@mail.each do |email|%>
    <tr>
      <th scope="row"><%=email.id%></th>
      <td><%=email.object%></td>
      <td><%=email.body%>/td>
    </tr>
    <%end%>
  </tbody>
</table>
